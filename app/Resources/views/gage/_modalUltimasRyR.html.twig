<script>
        function ultimasRyR(gageId, ultimasRyRPath) {
        
            
        $.ajax({
            type: "GET",
            url: ultimasRyRPath,
            data: {},
            success: function (datos) {
                $("#ryr-name-header-modal-view").html("Instrumento #" + gageId);
                $("#ryr-text-header-body-view").html(datos);
                $('#ultimasRyRModal').modal('show');
            },
            error: function (jqXHR, textStatus, errorThrown) {
                alert(jqXHR.responseJSON.message);
            }
        });
    };
</script>
