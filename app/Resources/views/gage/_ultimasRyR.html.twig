<table class="table table-hover table-sm" style="font-size: 14px;">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fecha</th>
            <th>Tipo</th>
            <th>Comentario</th>
            <th>PFD</th>
        </tr>
    </thead>
    <tbody>
        {% for conten in contenido %}
        <tr>
            <td>{{conten.id }}</td>
            <td>{{conten.fecha|date('Y-m-d')}}</td>
            <td>{{conten.ryRTipo.descripcion}}</td>
            <td>{{conten.memo}}</td>
            <td>{%- if conten.soloPdf %}
                            {% if conten.rutaArchivo is not empty %}
                            <a  href="#" id="pdf" onclick="mypdf(encodeURI('{{ conten.rutaArchivo|replace('\\', '//') }}'))"><i class="fas fa-file-pdf"></i></a>
                            {% endif %}
                        {% else %}
                        {% endif %}
            </td>
        </tr>        
        {% endfor %} 
    </tbody>
</table>
